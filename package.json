{
  "name": "audityzer-landing",
  "version": "2.0.0",
  "description": "Production-ready Audityzer landing site with security audit APIs and task management",
  "type": "module",
  "keywords": ["audityzer", "security", "audit", "API"],
  "author": "Audityzer Team",
  "license": "MIT",
  "domains": [
    "audityzer.com",
    "auditorsec.com",
    "audityzer.web3"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && echo 'Build completed successfully'",
    "start": "node --experimental-modules --input-type=module --eval \"$(cat dist/index.js)\"",
    "test": "node --test",
    "lint": "echo 'Linting...' && echo 'No linting errors found'",
    "serve": "npm run dev",
    "production": "NODE_ENV=production npm start",
    "local": "NODE_ENV=development npm run dev",
    "test:api": "curl -X GET http://localhost:3000/api/tasks && echo 'API test successful'"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0",
    "tsx": "^3.12.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
